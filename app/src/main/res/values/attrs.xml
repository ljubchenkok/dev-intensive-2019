        <attr name="img_roundedCorner" format="dimension" />
        <attr name="img_strokeColor" format="color" />
        <attr name="img_strokeWidth" format="dimension" />
    </declare-styleable>


    <attr name="colorAccentedSurface" format="reference"/>
