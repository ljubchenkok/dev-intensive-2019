<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AspectRatioImageView">
        <attr name="aspectRatio" format="float"/>
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="cv_borderWidth" format="dimension"/>
        <attr name="cv_borderColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="AvatarImageView">
        <attr name="img_drawImageReplace" format="boolean" />
        <attr name="img_foregroundColorPressed" format="color" />
