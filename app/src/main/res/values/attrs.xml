    <attr name="colorItemDate" format="reference"/>
    <attr name="colorItemIndicator" format="reference"/>
    <attr name="colorItemCount" format="reference"/>
    <attr name="colorSwipeBackground" format="reference"/>
    <attr name="colorArchiveBackground" format="reference"/>
    <attr name="colorSelectedItemBackground" format="reference"/>
    <attr name="colorSelectedUserItem" format="reference"/>
    <attr name="colorChipGroup" format="reference"/>
    <attr name="colorChip" format="reference"/>
    <attr name="colorSnackBarBackground" format="reference"/>
    <attr name="colorSnackBarText" format="reference"/>

